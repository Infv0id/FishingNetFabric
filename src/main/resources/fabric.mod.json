{
	"schemaVersion": 1,
	"id": "fishingnetfabric",
	"version": "${version}",
	"name": "Fishing Net Mod Fabric",
	"description": "This is a Port of the Fishing Net Mod from NeoForge to Fabric.",
	"authors": [
		"Me!"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
